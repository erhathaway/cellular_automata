{
    "extends": "airbnb",
    "rules": {
      "no-underscore-dangle": [2, { "allowAfterThis": true }],
      "implicit-arrow-linebreak": 0,
      "react/jsx-filename-extension": 0,
      "babel/new-cap": 1,
      "babel/no-invalid-this": 1,
      "babel/object-curly-spacing": 0,
      "babel/quotes": 0,
      "babel/semi": 1,
      "babel/no-unused-expressions": 1
    },
    "globals": {
      "window": true,
      "document": true
    },
    "plugins": [
      "babel"
    ]
}
